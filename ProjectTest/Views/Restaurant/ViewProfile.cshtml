@model ProjectTest.Models.UserRestaurant
@{
    ViewBag.Title = "ViewProfile";
    Layout = "~/Views/Shared/_AfterLoginRestaurant.cshtml";
}

<h3>My Profile</h3>
<br />
<img src="@Model.Picture" alt="Alternate Text" height="150" width="150" class="img-rounded" />
<br /> <br />
<p><b>Name:</b> @Model.Name</p>
<p><b>Email:</b>@Model.Email</p>
<p><b>Location:</b>@Model.Location</p>
<p><b>Cuisine:</b>@Model.Restaurant_Type</p>

